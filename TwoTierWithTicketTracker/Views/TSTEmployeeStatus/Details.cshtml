@model DataLayer.TSTEmployeeStatus

@{
    ViewBag.Title = " Employee Status Details";
}
<div class="box" style="overflow-x:auto;">
    <h2 class="headingHeader text-center">@ViewBag.Title</h2>

    <div>
        
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.EmployeeStatusName)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.EmployeeStatusName)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Description)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Description)
            </dd>

        </dl>
    </div>
    <p>
        @Html.ActionLink("Edit Status", "Edit", null,  new { id = Model.EmployeeStatusID }, new { @class = "btn btn-danger" })
        @Html.ActionLink("Back to Status", "Index",null, new { @class = "btn btn-primary" })
    </p>
</div>