@model DataLayer.TSTOrderPriority

@{
    ViewBag.Title = "Priority Details";
}


<div class="box" style="overflow-x:auto;">
    <h2 class="headingHeader text-center">@ViewBag.Title</h2>
    <div>

        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.Name)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Name)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Description)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Description)
            </dd>

        </dl>
    </div>
    <p class="text-center">
        @Html.ActionLink("Edit Priority", "Edit",null, new { id = Model.PriorityID }, new { @class = "btn btn-danger" })
        @Html.ActionLink("Back to Priorities", "Index", null, new { @class = "btn btn-primary" })
    </p>
</div>