@model DataLayer.TSTOrderStatus

@{
    ViewBag.Title = "Order Status Details";
}
<div class="box" style="overflow-x:auto;">
    <h2 class="headingHeader text-center">@ViewBag.Title</h2>

    <div>
        
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.Name)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Name)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Description)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Description)
            </dd>

        </dl>
    </div>
    <p>
        @Html.ActionLink("Edit Status", "Edit",null, new { id = Model.TicketStatusID }, new { @class = "btn btn-danger" })
        @Html.ActionLink("Back to Statuses", "Index", null, new { @class = "btn btn-primary" })
    </p>
</div>